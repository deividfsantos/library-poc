      "response": {
        "status": 417,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "message": "User already exists."
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          }
        }
      }
    }
  ],
