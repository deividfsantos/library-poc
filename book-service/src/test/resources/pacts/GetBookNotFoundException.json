{
  "consumer": {
    "name": "Book-Api"
  },
  "provider": {
    "name": "book-service"
  },
  "interactions": [
    {
      "description": "a request to GET an invalid book",
      "providerState": "get a book not found by name",
      "request": {
        "method": "GET",
        "path": "/v1/books/Harry Potter",
        "headers": {
          "Content-Type": "application/json"
        }
      },
      "response": {
        "status": 417,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "message": "No books with this name were found."
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}